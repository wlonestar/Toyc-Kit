extern void printI64(i64 val);

i64 factorial(i64 val) {
  if (val == 1 || val == 0) {
    return 1;
  } else {
    return factorial(val - 1) * val;
  }
}

i64 main() {
  i64 a = 0;
  while (a <= 20) {
    printI64(factorial(a));
    a = a + 1;
  }
  for (i64 i = 0; i <= 20; i = i + 1) {
    printI64(factorial(i));
  }
  return 0;
}
