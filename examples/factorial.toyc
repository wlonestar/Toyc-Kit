extern void println();
extern void printSpace();
extern void printI64(i64 val);

i64 factorial(i64 val) {
  if (val == 1 || val == 0) {
    return 1;
  }
  return factorial(val - 1) * val;
}

i64 main() {
  i64 a = 0;
  while (a <= 10) {
    printI64(factorial(a));
    printSpace();
    a = a + 1;
  }
  return 0;
}
